{
  "name": "掘金头像框",
  "description" : "自定义掘金头像框",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "icon.png",
    "default_title": "掘金头像框"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["https://juejin.cn/"],
  "background": {
    "service_worker": "background.js"
  }
}
